# Drift Benchmark Configuration
# Copy this file to .env and modify as needed

# Directory Settings
# Use absolute paths for custom locations, or relative paths from project root
DRIFT_BENCHMARK_COMPONENTS_DIR=components
DRIFT_BENCHMARK_CONFIGURATIONS_DIR=configurations
DRIFT_BENCHMARK_DATASETS_DIR=datasets
DRIFT_BENCHMARK_RESULTS_DIR=results
DRIFT_BENCHMARK_LOGS_DIR=logs

# Application Settings
DRIFT_BENCHMARK_LOG_LEVEL=INFO

# Performance Settings
DRIFT_BENCHMARK_ENABLE_CACHING=true
DRIFT_BENCHMARK_MAX_WORKERS=4

# Execution Settings
DRIFT_BENCHMARK_RANDOM_SEED=42

# Memory Settings
DRIFT_BENCHMARK_MEMORY_LIMIT_MB=4096

# Example custom configurations:
# DRIFT_BENCHMARK_COMPONENTS_DIR=/home/user/custom/components
# DRIFT_BENCHMARK_LOG_LEVEL=DEBUG
# DRIFT_BENCHMARK_MAX_WORKERS=8
# DRIFT_BENCHMARK_MEMORY_LIMIT_MB=8192
# DRIFT_BENCHMARK_RANDOM_SEED=123
